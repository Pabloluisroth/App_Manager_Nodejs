<%- include("template/header") %>
<%- include("template/navbar") %>
<%- include("template/aside") %>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <%- include('template/breadcrumb', {module: 'Completed Tasks'}) %>
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-md-12">            
            <div class="container">
              <table id="example1" class="table table-bordered table-striped text-center mt-4">
                <thead>
                  <tr class="bg-primary text-white">
                    <th scope="col">#ID</th>
                    <th scope="col">Titulo</th> 
                    <th scope="col">Estado</th> 
                    <th scope="col">Acciones</th> 
                  </tr>
                </thead>
                <tbody>
                  <% results.forEach ((task) => { %>
        
                    <tr>
                      <th><%= task.id %></th>
                      <th><%= task.titulo%></th>
                      <th><%= task.estado%></th>
                      <td>
                        <!--

                        <a href="//<%= task.id %>" onclick="confirmationRevertTask(<%= taks.id %>);" class="btn btn-info"> <i class=" fa fa-eraser"></i></a>               
                        <a href="//<%= task.id %>" onclick="confirmation(<%= task.id %>);" class="btn btn-danger"> <i class="fa-regular fa-trash-can"></i></a> 
                        -->
                        <a onclick="confirmationRevertTask(<%= taks.id %>);" class="btn btn-info"> <i class=" fa fa-eraser"></i></a>               
                        <a onclick="confirmation(<%= task.id %>);" class="btn btn-danger"> <i class="fa-regular fa-trash-can"></i></a> 
                      </td>
                    </tr>                              
                  <% }) %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>

    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
  
  <%- include("template/footer") %>